{
  "manifest_version": 2,
  "name": "d20pfsrd search",
  "version": "1.1",

  "description": "Search on the d20pfsrd page directly from the browser bar.",

  "icons": {
    "256": "icons/icon-48.png"
  },

  "chrome_settings_overrides": {
    "search_provider": {
      "name": "d20pfsrd",
      "keyword": "@d20",
      "favicon_url": "favicon.png",
      "search_url": "https://cse.google.com/cse?cx=006680642033474972217%3A6zo0hx_wle8&q={searchTerms}",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }
}
